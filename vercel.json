{
  "version":2,
  "builds":[
    {
      "src":"./api/index.js",
      "use":"@vercel/node"
    }
  ],
  
  "buildCommand": "next build",
"crons": [
      {
        "path": "/api/index.js",
        "schedule": "* * * * *"
      }
     
    ],
    
      "devCommand": "next dev",
    
  
  "routes":[
    {
      "src":"/(.*)",
      "dest":"/"
    }
  ],
 
  "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }]
}
